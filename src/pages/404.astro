---
import Layout from '../layouts/Layout.astro';
---
<Layout title="404 Page not found">
    <div class="row">
        <section class="section content">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <h2>Sorry, we couldn't find that page!</h2>
                        <p>We recently redesigned our web site, so you may have followed a link that no longer exists.</p>
                        <p>You can try <a href="/search">searching our site and collections.</a></p>
                        <p>If you still can't find what you're looking for, please <a href="/about/contact-connect">Contact Us</a></p>
                    </div>
                </div>
                <div class="row">
                <p></p>
                </div>
            </div>
        </section>
    </div>
  </Layout>