---
import { storyblokEditable } from '@storyblok/astro';
import { richTextResolver } from '@storyblok/richtext';
import DigitalCollectionsUnionSearch from './DigitalCollectionsUnionSearch.astro';

export interface Props {
	blok: {
    content: any;
  };
}

const { blok } = Astro.props
const { render } = richTextResolver()
const html = blok.content ? render(blok.content) : ''
---

<div {...storyblokEditable(blok)} set:html={html} />
